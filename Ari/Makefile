all:: $(CLIENT)

$(CLIENT): $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

saber_$(CLIENT):: $(SRCS)
	# load $(ALLDEFINES) $(SRCS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

osaber_$(CLIENT):: $(OBJS)
	# load $(ALLDEFINES) $(OBJS) $(LOCAL_LIBRARIES) $(SYS_LIBRARIES) $(EXTRA_LIBRARIES)

install:: $(CLIENT)
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  $(CLIENT) $(DESTDIR)$(BINDIR)

install.man:: $(CLIENT).man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) $(CLIENT).man $(DESTDIR)$(MANDIR)/$(CLIENT).$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

install:: $(CLASS)
	$(INSTALL) -c $(INSTAPPFLAGS) $(CLASS) $(DESTDIR)$(XAPPLOADDIR)/$(CLASS)

install::
	@if [ -d $(DESTDIR)$(XAPPLOADDIR)/$(CLIENT) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)/$(CLIENT)); fi

install:: $(APPDEFS)
	@if [ -d $(DESTDIR)$(XAPPLOADDIR)/$(CLIENT) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(XAPPLOADDIR)/$(CLIENT)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(APPDEFS); do \
	(set -x; $(INSTALL) -c $(INSTAPPFLAGS) $$i $(DESTDIR)$(XAPPLOADDIR)/$(CLIENT)); \
	done

all:: Test

Test.c: Ari.c
	rm -f Test.c ; ln Ari.c Test.c

Test.o: Test.c
	$(RM) $@
	$(CC) -c $(CFLAGS) $(DEBUG_CLIENT_OPTIONS) $*.c

Test: Test.o $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ Test.o $(LDOPTIONS) $(DEBUG_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) Test

clean::
	rm -f Test.o

