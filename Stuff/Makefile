# $XConsortium: Library.tmpl,v 1.12 92/03/20 15:05:19 rws Exp $

         CC = cc
  CCOPTIONS =
STD_DEFINES =
CDEBUGFLAGS = -O

   INSTALLFLAGS = $(INSTINCFLAGS)
       LINTLIBS = $(LINTXLIB)

HEADERS = Stuff.h
   SRCS = Stuff.c
   OBJS = Stuff.o

   LIBRARY_NAME = Stuff

# # Wcl Library Template for X11R5
# #

      LIBRARY_g = $(LIBRARY_NAME)_g
      LIBRARY_p = $(LIBRARY_NAME)_p

all::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

includes::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

.c.o:
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(RM) $@
	$(CC) -c $(CFLAGS) $(_NOOP_) $(SHLIBDEF) $(SHAREDCODEDEF) $(PICFLAGS) $*.c

clean::
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

all:: lib$(LIBRARY_NAME).so.$(SOWCREV)

lib$(LIBRARY_NAME).so.$(SOWCREV): $(OBJS)
	$(RM) $@~
	(cd .; $(LD) -o ./$@~ $(SHLIBLDFLAGS) $(OBJS) $(REQUIREDLIBS))
	$(RM) $@
	$(MV) $@~ $@

clean::
	$(RM) lib$(LIBRARY_NAME).so.$(SOWCREV)

lintlib:: llib-l$(LIBRARY_NAME).ln

llib-l$(LIBRARY_NAME).ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)$(LIBRARY_NAME) $(LINTFLAGS) $(SRCS)

includes::
	@if [ -d $(BUILDINCDIR)/$(LIBRARY_NAME) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(BUILDINCDIR)/$(LIBRARY_NAME)); fi
	@(set -x; cd $(BUILDINCDIR)/$(LIBRARY_NAME); for i in $(HEADERS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)/../$(CURRENT_DIR)/$$i .; \
	done)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

# # End of Wcl Library Template for X11R5

includes::

install::

install.man::

